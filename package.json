{
	"name": "pota",
	"description": "A Runtime HTML Renderer for Reactive Libraries.",
	"version": "0.1.0",
	"author": "Tito Bouzout",
	"type": "module",
	"exports": {
		".": "./src/index.js",
		"./flimsy": "./src/reactivity/flimsy.js",
		"./solid": "./src/reactivity/solid.js",
		"./oby": "./src/reactivity/oby.js",
		"./*": "./*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/titoBouzout/pota"
	},
	"prettier": {
		"printWidth": 70,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "avoid",
		"proseWrap": "never",
		"endOfLine": "lf",
		"singleAttributePerLine": true
	}
}
