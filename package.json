{
  "name": "pota",
  "description": "A Reactive HTML Renderer.",
  "version": "0.1.1",
  "author": "Tito Bouzout",
  "type": "module",
  "types": "./pota.d.ts",
  "main": "./src/@main.js",
  "exports": {
    ".": "./src/@main.js",
    "./jsx-runtime": "./src/renderer/jsx-runtime.js",
    "./babel-preset": "./babel-preset/index.cjs",
    "./hooks": "./src/hooks/@main.js",
    "./router": "./src/components/router/@main.js"
  },
  "imports": {
    "#main": "./src/@main.js",
    "#primitives": "./src/lib/reactivity/primitives/solid.js",
    "#primitives/*": "./src/lib/reactivity/primitives/*",
    "#comp": "./src/lib/comp/@main.js",
    "#std": "./src/lib/std/@main.js",
    "#reactivity": "./src/lib/reactivity/@main.js",
    "#urls": "./src/lib/urls/@main.js",
    "#scroll": "./src/lib/scroll/@main.js",
    "#time": "./src/lib/time/@main.js",
    "#hooks": "./src/hooks/@main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/titoBouzout/pota"
  },
  "prettier": {
    "printWidth": 70,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "endOfLine": "lf",
    "singleAttributePerLine": true
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "babel-plugin-transform-rename-import": "^2.3.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.22.11"
  }
}
